{
  "name": "prisma-generator-nestjs-swagger",
  "description": "generates basic dto classes for use with @nestjs/swagger and class-validator",
  "version": "1.0.7",
  "license": "MIT",
  "bin": "./dist/prisma-generator-nestjs-swagger.js",
  "engines": {
    "node": ">=14.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npx spack && yarn build:shebang && yarn build:chmod",
    "build:chmod": "chmod +x ./dist/prisma-generator-nestjs-swagger.js",
    "build:shebang": "echo \"#!/usr/bin/env node\"|cat - ./dist/prisma-generator-nestjs-swagger.js > /tmp/out && mv /tmp/out ./dist/prisma-generator-nestjs-swagger.js",
    "dev": "npx tsc -w",
    "format": "prisma format --schema=./public.prisma",
    "gen": "prisma generate --schema=./public.prisma",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@prisma/generator-helper": "^4.10.1",
    "@prisma/internals": "^4.10.1",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "@swc/core": "^1.3.36",
    "@swc/cli": "^0.1.62",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0"
  },
  "devDependencies": {
    "@nestjs/swagger": "^6.2.1",
    "@prisma/client": "^4.10.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "17.0.21",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "eslint": "^8.32.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "prisma": "^4.10.1",
    "typescript": "^4.9.5"
  },
  "author": "dayne.mentier@gmail.com",
  "keywords": [
    "@nestjs/swagger",
    "class-validator",
    "prisma",
    "generator"
  ],
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "strict",
    "jsxSingleQuote": true,
    "importOrder": [
      "^@(.*)/(.*)$",
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ],
    "importOrderCaseInsensitive": true,
    "importOrderParserPlugins": [
      "typescript",
      "decorators-legacy"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "insertPragma": false,
    "quoteProps": "as-needed",
    "requirePragma": false,
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
}
